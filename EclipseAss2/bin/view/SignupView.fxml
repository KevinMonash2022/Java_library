<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="400.0" maxWidth="400.0" minHeight="400.0" minWidth="400.0" prefHeight="400.0" prefWidth="400.0" spacing="10.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label prefHeight="17.0" prefWidth="250.0" text="Username" />
      <TextField fx:id="username" maxWidth="250.0">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin></TextField>
      <Label prefWidth="250.0" text="Firstname" />
      <TextField fx:id="firstName" maxWidth="250.0" />
      <Label prefWidth="250.0" text="Lastname" />
      <TextField fx:id="lastName" maxWidth="250.0" />
      <Label prefWidth="250.0" text="Password" />
      <PasswordField fx:id="password" maxWidth="250.0">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </PasswordField>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="400.0" spacing="20.0">
         <children>
            <Button fx:id="createUser" mnemonicParsing="false" text="Create user" />
            <Button fx:id="close" mnemonicParsing="false" text="Close" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <Label fx:id="status" prefHeight="27.0" prefWidth="351.0">
         <VBox.margin>
            <Insets />
         </VBox.margin></Label>
   </children>
   <padding>
      <Insets top="20.0" />
   </padding>
</VBox>
